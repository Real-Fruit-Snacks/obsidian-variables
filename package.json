	"license": "MIT",
	"bugs": {
		"url": "https://github.com/yourusername/obsidian-variables/issues"
	},
	"homepage": "https://github.com/yourusername/obsidian-variables#readme",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.5",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
